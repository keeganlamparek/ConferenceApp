<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.keegan.conferenceapp.KentFloorSelectorActivity">

    <Button
        android:id="@+id/firstFloorButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contextClickable="false"
        android:text="Kent First Floor Map"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="102dp" />

    <Button
        android:id="@+id/secondFloorButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kent Second Floor Map"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="297dp" />

</android.support.constraint.ConstraintLayout>
